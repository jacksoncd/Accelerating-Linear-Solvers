{
    "name": "Xilinx General Linear Solver", 
    "description": "", 
    "flow": "vitis", 
    "platform_type": "pcie", 
    "platform_whitelist": [
        "u250", 
        "u200"
    ], 
    "platform_blacklist": [
        "zc"
    ], 
    "platform_properties": {
        "u250": {
            "host": {
                "compiler": {
                    "symbols": [
                        "USE_DDR"
                    ]
                }
            }, 
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u250.ini"
                    ]
                }
            }
        }, 
        "u200": {
            "host": {
                "compiler": {
                    "symbols": [
                        "USE_DDR"
                    ]
                }
            }, 
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u200.ini"
                    ]
                }
            }
        }
    }, 
    "launch": [
        {
            "cmd_args": " -xclbin BUILD/kernel_gelinearsolver.xclbin", 
            "name": "generic launch for all flows"
        }
    ], 
    "host": {
        "host_exe": "test_gelinearsolver.exe", 
        "compiler": {
            "sources": [
                "test_gelinearsolver.cpp", 
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ], 
            "includepaths": [
                "LIB_DIR/ext/xcl2", 
                "LIB_DIR/ext/MatrixGen"
            ], 
            "options": "-O3 "
        }
    }, 
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L2/include", 
                "LIB_DIR/ext"
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "location": "kernel_gelinearsolver.cpp", 
                    "frequency": 300.0, 
                    "clflags": " -D KERNEL_NAME=kernel_gelinearsolver_0", 
                    "name": "kernel_gelinearsolver_0", 
                    "num_compute_units": 1, 
                    "compute_units": [
                        {
                            "name": "kernel_gelinearsolver_0_1", 
                            "slr": "SLR0", 
                            "arguments": [
                                {
                                    "name": "dataA", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "dataB", 
                                    "memory": "DDR[0]"
                                }
                            ]
                        }
                    ]
                }
            ], 
            "frequency": 300.0, 
            "name": "kernel_gelinearsolver"
        }
    ], 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": 32768, 
                "max_time_min": 120
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu", 
            "vitis_hw"
        ], 
        "category": "canary"
    }, 
    "gui": true
}